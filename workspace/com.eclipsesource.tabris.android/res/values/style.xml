        <item name="android:textSize">18dp</item>
    </style>

    <style name="TextAppearance.Holo.Widget.PopupMenu.Small">
        <item name="android:textSize">14dp</item>
    </style>

    <style name="TextAppearance.Holo.Widget" parent="TextAppearance.Widget" />

